{
  "name": "petscii-paint",
  "version": "1.0.0",
  "description": "A PETCSII paint application",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roberteliason/petscii-paint.git"
  },
  "keywords": [
    "paint",
    "commmodore"
  ],
  "author": "Robert Eliason",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/roberteliason/petscii-paint/issues"
  },
  "homepage": "https://github.com/roberteliason/petscii-paint#readme",
  "dependencies": {
    "p5": "^0.10.2"
  },
  "scripts": {
    "serve": "NODE_ENV=development node_modules/http-server/bin/http-server ./",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "devDependencies": {
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "http-server": "^14.1.1",
    "laravel-mix": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2"
  }
}
